{
  "schemaVersion": 1,
  "id": "pipimod",
  "version": "${version}",

  "name": "PipiMod",
  "description": "Adds the great \"pipi\" mob to the game, truly something we all strive to have in life",
  "authors": [
    "fake_orchestra",
    "GoopSwagger"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/pipimod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.goopswagger.pipimod.PipiMod"
    ],
    "client": [
      "com.goopswagger.pipimod.PipiModClient"
    ]
  },
  "mixins": [
    "pipimod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.14",
    "fabric-api": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
